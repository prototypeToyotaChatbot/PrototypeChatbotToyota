POSTGRES_HOST=toyota-vector-store
POSTGRES_USER=admin
POSTGRES_PASSWORD=password

POSTGRES_DB_DEFAULT=postgres
DATABASE_URL_DEFAULT=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:5432/${POSTGRES_DB_DEFAULT}


POSTGRES_DB_CAR=infinity_car_db
DATABASE_URL_CAR=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:5432/${POSTGRES_DB_CAR}

CAR_SERVICE_URL=http://car_service:8007

N8N_ENCRYPTION_KEY=super-secret-key
N8N_USER_MANAGEMENT_JWT_SECRET=even-more-secret
N8N_DEFAULT_BINARY_DATA_MODE=filesystem

SECRET_KEY=${N8N_USER_MANAGEMENT_JWT_SECRET}
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=720

NG_CRE=--
NG_URL=--

PGADMIN_DEFAULT_EMAIL=ikan@garudainfinity.com
PGADMIN_DEFAULT_PASSWORD=asdf

APP_NETWORK = infinity_net

# Port yang dibuka ke publik
HOST_PORT=2323

# Port yang dipakai di dalam container (sesuai --port)
CONTAINER_PORT=2323

# Path folder lokal yang berisi data untuk MCP
GATE_FOLDER=./supergateFolder

GENERIC_TIMEZONE=Asia/Jakarta

# =================================================================
# CHATBOT CONFIGURATION - Anda bisa mengganti konfigurasi di sini
# =================================================================

# N8N Webhook untuk AI Chatbot (ganti dengan URL webhook N8N Anda)
N8N_CHATBOT_WEBHOOK_URL=https://extrasensory-inflatedly-sherrie.ngrok-free.dev/webhook/get-message

# Enable/disable AI processing (true/false)
USE_AI_PROCESSING=true

# Timeout untuk webhook request (detik)
WEBHOOK_TIMEOUT=30

# Pesan-pesan chatbot (bisa diganti sesuai kebutuhan)
WELCOME_MESSAGE=Selamat datang! Saya adalah asisten konsultasi Toyota Anda. Saya dapat membantu menemukan mobil yang sesuai dengan kebutuhan Anda, membandingkan berbagai model, memberikan informasi promo terbaru, dan layanan lainnya. Apa yang dapat saya bantu hari ini?

FALLBACK_MESSAGE=Dengan senang hati saya akan membantu Anda terkait kebutuhan otomotif Toyota. Mulai dari pencarian mobil, informasi promo, perbandingan model, hingga rekomendasi berdasarkan preferensi Anda.

ERROR_MESSAGE=Maaf, saat ini sedang terjadi kendala teknis. Silakan coba kembali beberapa saat lagi atau hubungi langsung tim penjualan Toyota kami untuk mendapatkan bantuan.


N8N_FEATURE_FLAG_MCP=true
